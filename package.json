{
  "name": "bun-template",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run index.ts",
    "bundle": "rm -rf build/*.* && bun run bundler/bundler.ts",
    "list": "bun run samples/list-scripts.tsx",
    "example": "bun run bundle && cd example && bun start && cd ..",
    "fileSample": "bun run samples/file.tsx && cat samples/data/test.json",
    "httpSample": "bun run samples/server.tsx"
  },
  "files": [
    "resources"
  ],
  "devDependencies": {
    "typescript": "^5.2.2",
    "@types/react": "^18.2.21",
    "bun-types": "latest",
    "png-to-ico": "^2.1.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "gl-matrix": "^3.4.3"
  }
}
